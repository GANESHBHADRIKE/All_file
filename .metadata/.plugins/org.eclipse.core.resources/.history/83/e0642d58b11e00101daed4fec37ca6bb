package programs;

public class PrimeNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num = 645;
		int prime = num%2;
		System.out.println(prime);
		if(prime == 1) {
			System.out.println("prime");
		}
		else {
			System.out.println("even");
		}
		
			
		}

	

}
