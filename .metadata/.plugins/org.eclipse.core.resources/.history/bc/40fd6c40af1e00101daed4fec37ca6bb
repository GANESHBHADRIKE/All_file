package programs;

public class ReverseString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String str = "Ganesh";
		System.out.println(str.length());
		for(int i = str.length() - 1; i >= 0; i--) {
			char ch = str.charAt(i);
			System.out.println(ch);
		}
	}

}
