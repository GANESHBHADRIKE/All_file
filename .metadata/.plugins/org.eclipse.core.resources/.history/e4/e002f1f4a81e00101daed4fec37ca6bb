package programs;

public class FindCharFromString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str = "Ganesh";
		char tofind = 'h';
		
		for(int i = 0;i<str.length();i++) {
			
			if(str.charAt(i) == tofind) {
				System.out.println("Character found at"+i);
			}
		}

	}

}
