package practical_notes;

public class Practice2 {
    public static void main(String[] args) {
        int[] lts = {1,2,3,4};
        		
        		for(int i : lts) {
        			if(i==4) {
        				System.out.println("found"+i);
        				
        			}
        			
        		}
        		
        		
       
    }
}