package oppsJava;

public class ClassandObject {
	
	
		
		int x ;			// Attribute
		
		public void MyFunction() {		//	Function or Method
			
			System.out.println("This is my Function");
		}
	

	public static void main(String[] args) {

		ClassandObject obj = new ClassandObject();
		
		obj.MyFunction();
		
	}

	

}
